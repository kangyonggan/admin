{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init": "npm install",
    "dev": "vue-cli-service serve",
    "mock": "vue-cli-service serve --mode mock",
    "hd": "vue-cli-service serve --mode hd",
    "build": "vue-cli-service build",
    "deploy": "ssh ubuntu@kangyonggan.com 'rm -rf /home/ubuntu/apps/admin/*';scp -r dist/* ubuntu@kangyonggan.com:/home/ubuntu/apps/admin/",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "mavon-editor": "^2.7.7",
    "qs": "^6.9.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-router": "^2.8.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "mockjs": "^1.0.1-beta3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": [
        "off"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
